# Ignore Python cache and compiled files
__pycache__/
*.pyc
*.pyo
*.pyd
app.cpython-*.pyc

# Ignore environment files
.env

# Ignore log files
*.log

# Ignore datasets (optional, if not needed in the container)
dataset/
*.csv

# Ignore model artifacts (optional, if not needed in the container)
*.pkl

# Ignore node modules (to prevent unnecessary copy; should be installed inside container)
node_modules/

# Ignore uploaded files (if they are user-generated)
uploads/

# Ignore Git and system files
.git/
.gitignore
.DS_Store
Thumbs.db

# Ignore Docker-related files that should not be copied
.dockerignore
docker-compose.yml
